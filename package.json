  "scripts": {
    "start": "node index.js",
    "start:test": "npm run server & npm run test:watch",
    "build":
      "webpack --mode production --profile --json > stats.json --config webpack/webpack.prod.js",
    "dev":
      "webpack-dev-server --mode development --open --config webpack/webpack.dev.js",
    "postinstall": "npm run build",
    "lint": "eslint --ext .jsx --ext .js src/",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "flow": "flow"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint & flow"
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/tests/config.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/src/tests/mocks/fileMock.js"
    }
  },
